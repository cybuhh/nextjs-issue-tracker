{
  "name": "nextjs-issue-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "ts-check": "tsc",
    "ts-watch": "tsc -w",
    "prisma:migrate": "prisma migrate dev",
    "prisma:format": "prisma format",
    "prettier": "prettier -c {app,prisma}/**/*.ts",
    "prettier:fix": "prettier -w {app,prisma}/**/*.ts",
    "cypress:open": "cypress open --component -b chrome",
    "cypress:run:component": "cypress run --component",
    "cypress:report:merge": "mochawesome-merge cypress/results/mochawesome*.json > cypress/results/mochawesome-bundle.json",
    "cypress:report:generate": "marge cypress/results/mochawesome-bundle.json -o cypress/results/html",
    "db:import": "tsc tests-e2e/initDb.ts && node tests-e2e/initDb.js ; rm tests-e2e/initDb.js prisma/client.js",
    "playwright": "npx playwright test",
    "playwright:ui": "npx playwright test --ui"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.10.2",
    "@radix-ui/themes": "^3.0.2",
    "@tailwindcss/typography": "^0.5.12",
    "@tanstack/react-query": "^5.28.14",
    "@testing-library/cypress": "^10.0.1",
    "classnames": "^2.5.1",
    "delay": "^6.0.0",
    "easymde": "^2.18.0",
    "msw": "^2.3.0",
    "next": "^14.1.3",
    "next-auth": "^4.24.7",
    "prisma": "^5.10.2",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.1",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.0.1",
    "react-loading-skeleton": "^3.4.0",
    "react-markdown": "^9.0.1",
    "react-simplemde-editor": "^5.2.0",
    "recharts": "^2.12.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.43.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "cypress": "^13.7.2",
    "eslint": "^8",
    "eslint-config-next": "^14.1.3",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
